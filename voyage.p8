pico-8 cartridge // http://www.pico-8.com
version 41
__lua__
grid_size=8

function _init()
  init_player()
end

function _update()
  dbg={}
  update_player()
end

function _draw()
  cls()
  map()
  draw_player()
  draw_debug()
end

function draw_debug()
  for i=1,#dbg do
    print(dbg[i],0,(i-1)*7,7)
  end 
end

-->8
-- tools

function cycle(arr,i,hz)
  hz=max(0,hz)  
  return arr[(flr((i*hz)%#arr)+1)]
end

function mfget(x,y,f)
  return fget(mget(flr(x/grid_size),flr(y/grid_size)),f)
end
-->8
-- player

function _init()
		pl_sprs={1,2,3,2}
  pl={
    x=grid_size,
    y=grid_size,
    mv_buff={},
    pos_offset={},
    sp=1,
    flp=false,
  }
end

function update_player()
  get_pl_move()

  if #pl.pos_offset>0 then
    deli(pl.pos_offset,#pl.pos_offset)
  else
		  if #pl.mv_buff>0 then
			   move_pl()
		  end
  end 
end

function get_pl_move()
  local mv={dx=0,dy=0}
  
  if btnp(⬅️) then
    mv.dx=-1
  elseif btnp(➡️) then
    mv.dx=1
  elseif btnp(⬇️) then
    mv.dy=1
  elseif btnp(⬆️) then
    mv.dy=-1
  else
    mv=nil
  end
  
  if mv then
    add(pl.mv_buff,mv)
  end
end

function move_pl()
  local mv=pl.mv_buff[1]
  local dx,dy=mv.dx*grid_size,mv.dy*grid_size

  local nx=pl.x+dx
  local ny=pl.y+dy

  if mfget(nx,ny,flag_canwalk) then
		  pl.x=nx
		  pl.y=ny
    pl_walk(mv)
  else
    pl_hit_wall(mv)
  end

  if (mv.dx!=0) pl.flp=(mv.dx<0)

  deli(pl.mv_buff,1)
end

function pl_walk(mv)
  for i=0,-grid_size,-1 do
    add(pl.pos_offset,{dx=mv.dx*i,dy=mv.dy*i})
  end
end

function pl_hit_wall(mv)
  for i=0,flr(grid_size/2),1 do
    add(pl.pos_offset,{dx=mv.dx*i,dy=mv.dy*i})
  end
  for i=flr(grid_size/2),0,-1 do
    add(pl.pos_offset,{dx=mv.dx*i,dy=mv.dy*i})
  end
end

function draw_player()
  local x=pl.x
  local y=pl.y

  if #pl.pos_offset>0 then
    local os=pl.pos_offset[#pl.pos_offset]
    x+=os.dx
    y+=os.dy
    pl.sp+=1
  end

		spr(cycle(pl_sprs,pl.sp,0.5),x,y,1,1,pl.flp)
end

