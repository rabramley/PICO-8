-- utils
function blink_text(text,x,y,cols,hz)
  print(text,x,y,cycle(cols,time(),hz))
end

function blink_sprite(sprite,x,y,hz)
  if flr((time()*hz)%2)==0 then
    spr(sprite,x,y)
  end
end

function cycle(arr,i,hz)
  hz=max(0,hz)  
  return arr[(flr((i*hz)%#arr)+1)]
end

function collide(a,b)
  if max(a.x,b.x)>(min(a.x,b.x)+7) then
    return false
  elseif max(a.y,b.y)>(min(a.y,b.y)+7) then
    return false
  end

  return true
end

function debug(txt)
  print(txt,0,120)
end
