-- my god, it's full of stars
function init_starfield()
  stars={}
  
  for i=1,150 do
    add(
      stars,
      {
        x=flr(rnd(128)),
        y=flr(rnd(128)),
        spd=flr(rnd(99)),
       })
  end
  
  star_cols={5,13,6,7}
end

function update_starfield()
  for s in all(stars) do
      s.y+=s.spd/50
    s.y=s.y%128
  end
end

function draw_starfield()
  for s in all(stars) do
    pset(s.x,s.y,star_cols[flr(s.spd/20)])
  end
end
-->8
-- ship

function init_ship()
  flame_sprites={4,5,6,7}
  immune_time=100
  cool_time=10

  sh={
    x=64,
    y=110,
    spd=0,
    sprite=2,
    max_spd=12,
    manu=1,
    mzl=-1,
    immune=immune_time,
    cool=-1,
  }
end

function update_ship()
  if btn(⬅️) then
    sh.spd-=sh.manu
    sh.sprite=1
  elseif btn(➡️) then
    sh.spd+=sh.manu
    sh.sprite=3
  elseif sh.spd!=0 then
    sh.spd-=sgn(sh.spd)*sh.manu
    sh.sprite=2
  end
  sh.spd=mid(sh.max_spd,sh.spd,-sh.max_spd)
  sh.x+=sh.spd/4
  
  if (sh.x<-7) sh.x=127
  if (sh.x>127) sh.x=-7

  sh.mzl=max(-1,sh.mzl-1)
  sh.immune=max(-1,sh.immune-1)
  sh.cool=max(-1,sh.cool-1)
  
		if btn(❎) and sh.cool<0 and sh.immune<0 then
    sfx(0)
    add(buls,{x=sh.x,y=sh.y,sprite=0})
    sh.mzl=4
    sh.cool=cool_time
  end
end

function draw_ship()
  local sx=sh.x
  local sy=sh.y
  local ssp=sh.sprite
  local si=sh.immune
  local sm=sh.mzl
  local fsp=flame_sprite()
  local fy=sy+8
  
  if si>=0 then
    hz=8+flr(si/(immune_time/2))*-4,0
    blink_sprite(ssp,sx,sy,hz)
    blink_sprite(fsp,sx,fy,hz)
  else
    spr(ssp,sx,sy)
    spr(fsp,sx,fy)
  end
  
  if sm>=0 then
    local mlx=sx+2
    local mrx=sx+5
    local my=sh.y-1
    local smi=sm-2
    circfill(mlx,my,sm,10)
    circfill(mrx,my,sm,10)
    circfill(mlx,my,smi,7)
    circfill(mrx,my,smi,7)
  end
end

function flame_sprite()
  return cycle(flame_sprites,time(),30)
end
