-- console

function init_console()
  score=30000
  lives=3
  max_lives=3
end

function update_console()
end

function draw_console()
  -- user interface
  print("score: "..tostr(score),40,1,12)
  
  for i=0,max_lives-1 do
    if lives>i then
      spr(11,i*9,0)
    else
      spr(10,i*9,0)
    end
  end
end

-->8
-- enemies

function init_enemies()
  nmes={}
  nme_sprites={17,18,19,20,19,18}
  for i=1,rnd(10)+10 do
    add_enemy()
  end
end

function add_enemy()
  add(nmes,{
    x=flr(rnd(120))+3,
    y=flr(rnd(100))-100,
    spd=rnd(1),
    sprites=nme_sprites,
  })
end

function update_enemies()
  for i=1,rnd(10)+10-#nmes do
    add_enemy()
  end
  for e in all(nmes) do
    e.y+=e.spd
    
    if e.y>130 then
      del(nmes,e)
    end
  end
end

function draw_enemies()
  for e in all(nmes) do
    spr(cycle(e.sprites,time(),12),e.x,e.y)
  end
end

function update_collisions()
  if sh.immune>=0 then
    return
  end
  for e in all(nmes) do
    if collide(e,sh) then
      lives-=1
      sh.immune=immune_time
      sfx(2)
      del(nmes,e)
    end
    for b in all(buls) do
	    if collide(b,e) then
	      sfx(3)
	      del(nmes,e)
	      del(buls,b)
	    end
    end
  end
end


