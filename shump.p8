pico-8 cartridge // http://www.pico-8.com
version 41
__lua__
--main

function _init()
  mode="start"
end

function start_game()
  init_ship()
  init_enemies()
  init_bullets()
  init_starfield()
  init_console()
  --start_count()
  mode="game"
end

function start_count()
  count_start=time()
  count_sprites={64,66,68}
  count_sprite=1
  mode="count"
end

function _update()
  if mode=="game" then
    update_game()
  elseif mode=="start" then
    update_start()
  elseif mode=="over" then
    update_over()
  elseif mode=="count" then
    update_count()
  end
end

function update_game()
  update_starfield()
  update_ship()
  update_enemies()
  update_bullets()
  update_console()
  update_collisions()
end

function update_start()
  if btnp(4) or btnp(5) then
    start_game()
  end
end

function update_over()
  if btnp(4) or btnp(5) then
    mode="start"
  end
end

function update_count()
  count_sprite=flr(time()-count_start)+1
  
  if count_sprite>#count_sprites then
    mode="game"
  end
end

function _draw()
  if mode=="game" then
    draw_game()
  elseif mode=="start" then
    draw_start()
  elseif mode=="over" then
    draw_over()
  elseif mode=="count" then
    draw_count()
  end
end

function draw_game()
  cls()
  
  draw_starfield()
  draw_bullets()
  draw_enemies()
  draw_ship()
  draw_console()
  
  if lives<=0 then
    mode="over"
  end
end

function draw_start()
  cls(1)
  print("gallagher",45,40,12)
  blink_text("press any key to start",20,60,{1,7,7,7},2)
end

function draw_over()
  cls(2)
  print("game over",45,40,8)
  blink_text("press any key to continue",15,60,{2,4,8,8,8,9,10,9,8,8,8,4},16)
end

function draw_count()
  cls()
  spr(count_sprites[count_sprite],50,50,2,2)
end

