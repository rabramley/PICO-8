-- bullets
function init_bullets()
  buls={}
  
  bull_sprites={8,8,8,8,9}
end

function update_bullets()
  for b in all(buls) do
    b.y-=4
    b.sprite+=1
    if (b.y<0) del(buls,b)
  end
end

function draw_bullets()
  for b in all(buls) do
    spr(cycle(bull_sprites,time(),10),b.x,b.y)
  end
end

